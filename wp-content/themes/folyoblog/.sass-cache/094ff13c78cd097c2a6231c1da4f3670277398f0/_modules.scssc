:@template"û+/*-------------- logo ---------------*/
#logo{
	height:90px;
	width:90px;
	position:absolute;
	z-index:10;
	top:-13px;
	
	a{
		background:url("../images/logo.png") no-repeat;
		height:90px;
		width:90px;
		@include hide-text;
	}
}
/*-------------- date ---------------*/
.entry-date{
	width:30px;
	a, span{
		display:block;
		text-align:center;
		color:$beige;
		line-height:1;
	}
	.month{
		font-size:14px;
		font-weight:normal;
		text-transform:uppercase;
	}
	.day{
		font-size:28px;
		font-weight:bold;
	}
	.full-date{
		background:$lightgrey;
		opacity:0;
		@include transition-property("opacity, margin-top");
		@include transition-duration(300ms);
		@include transition-timing-function(ease-in);
		@include border-radius(10px);
		width:80px;
		margin-left:-35px;
		padding:10px;
		font-size:14px;
		font-weight:normal;
		position:relative;
		line-height:1.3;
		margin-top:0px;
	}
	.full-date:before{
		position:absolute;
		top:-5px;
		left:40px;
		content:" ";
		display:block;
		width: 0;
        height: 0;
        border-left: 10px solid transparent;
        border-right: 10px solid transparent;
        border-bottom: 10px solid $lightgrey;
	}
	&:hover .full-date{
		margin-top:10px;
		opacity:1;
		@include transition-property("opacity, margin-top");
	}
}
/*-------------- instapaper ---------------*/
.entry-instapaper{
	
}
/*-------------- follow ---------------*/
.entry-follow{
	margin-bottom:120px;
	h4{
		color:$brown2;
		font-size:14px;
	}
}
/*-------------- share ---------------*/
.entry-share{
	@include light-pattern;
	padding:15px 30px;
	overflow:hidden;
	width:630px;
	h3, h4{
		float:left;
		margin:0 30px 0 0;
		line-height:30px;
	}
	.share-option{
		float:left;
		margin-right:30px;
		.box{
			overflow:hidden;
			 .share{
				height:30px;
				width:30px;
				@include hide-text;
				float:left;
				margin-right:5px;
			}
			.count{
				float:right;
				width:60px;
				line-height:30px;
				height:30px;
				padding:0 10px 0 0;
				background:$lightbeige url("../images/pictos.png") no-repeat 0 -330px;
				@include border-radius(3px);
				text-align:right;
				font-size:16px;
			}
			&:hover .count{
				background-position:0 -330px;
				color:$brown;
			}
		}
	}
	#twitter{ 
		.share{
			background:url("../images/pictos.png") no-repeat 0 -120px;
		}
		&:hover .share{
			background-position:0 -150px;
		}
	}
	#facebook{
		.share{
			background:url("../images/pictos.png") no-repeat 0 -180px;
		
		}
		&:hover .share{
			background-position:0 -210px;
		}
	}
	#googleplus{
		.share{
			background:url("../images/pictos.png") no-repeat 0 -240px;
		}
		&:hover .share{
			background-position:0 -270px;
		}
	}
}
/*-------------- title ---------------*/
.entry-title{
	line-height:1;
	margin-bottom:10px;
	font-size:36px;
	margin-bottom:44px;
	a{
		color:$brown;
		&:visited{
			color:$brown2;
		}
		&:hover{
			color:$red;
		}
	}
}
/*-------------- entry content ---------------*/
.entry-content{
	margin-bottom:0px;
	>p:last-child:after{
		content:"‚ñ†";
		color:$red;
		display:inline-block;
		padding-left:3px;
		font-size:18px;
		line-height:15px;
	}
}
/*-------------- note ---------------*/
.entry-content h5, .note{
	background:$lightyellow;
	color:$darkyellow;
	font-weight:normal;
	padding:30px 30px 30px 60px;
	font-size:16px;
	line-height:1.7;
	position:relative;
	&:after{
		content:"Note";
		display:block;
		position:absolute;
		top:0;
		left:0;
		padding:2px 6px;
		text-transform:uppercase;
		font-weight:bold;
		background:#f9ec6e;
		font-size:12px;
	}
}
/*-------------- post ---------------*/
article.post, article.page{
	margin:$unit 0;
	padding: 0 $unit+30 0 0;
	position:relative;
	.entry-date{
		left:-90px;
		top:4px;
		position:absolute;
	}
	.read-more{
		text-decoration:underline;
		display:inline-block;
		margin-left:3px;
	}
}
#rss-link{
	@include hide-text;
	display:inline-block;
	height:12px;
	width:12px;
	background:url("../images/pictos.png") left -432px;
	margin:0 0 0 5px;
	cursor:pointer;
	&:hover, .current-menu-item &{
		background-position:left -420px;
	}
}
#tagline{
	font-size:16px;
	color:$brown2;
}
.social{
/*	h3{
		float:left;
		font-size:16px;
		line-height:30px;
		margin-right:12px;
	}*/
	ul{
		float:left;
		@include nav-ul;
		padding:0;
		margin:0;
		li{
			height:30px;
			width:30px;
			@include border-radius(30px);
			background:$red;
			margin-right:12px;
			&.find-me{
				width:auto;
				background:none;
				font-weight:bold;
				font-size:16px;
			}
			&:last-child{
				margin-right:0;
			}
			&:hover{
				background:$brown;
			}
			a{
				@include hide-text;
				height:30px;
			}
			&.twitter a{
				background:url("../images/pictos.png") no-repeat 0 -30px;
			}
			&.dribbble a{
				background:url("../images/pictos.png") no-repeat 0 -60px;
			}
			&.quora a{
				background:url("../images/pictos.png") no-repeat 0 -90px;
			}
			&.zerply a{
				background:url("../images/pictos.png") no-repeat 0 -120px;
			}
			&.blank{
				background:url("../images/pictos.png") no-repeat left top;
			}
		}
	}
}
#sidebar{
	@include letterpress(0.5);
	h3{
		text-transform:uppercase;
		color:$beige;
		font-weight:bold;
		font-size:18px;
		overflow:hidden;
		position:relative;
		height:21px;
		text-align:center;
		margin-bottom:15px;
		white-space:nowrap;
		span{
			display:inline-block;
			position:absolute;
			padding:0 10px;
			left:50%;
		}
	}
	h3 span:before, h3 span:after{
		background: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAQAAAAECAYAAACp8Z5+AAAAKklEQVQIHWP8//8/w5VD2/8zAIGOnScjEzIHJMgEkwHRIElGkBYYB0QDAAkQFU3J6bOAAAAAAElFTkSuQmCC");
		content:" ";
		display:block;
		height:14px;
		right:100%;
		width:300px;
		position:absolute;
		top:4px;
	}
	h3 span:after{
		left:100%;
	}
	.widgets{
		margin:0;
		padding:0;
		list-style-type:none;
		>li{
			margin-bottom:60px;
			line-height:1.5;
			font-size:16px;
		}
		.widget_recent_entries ul{
			margin:0;
			padding:0;
			list-style-type:none;
			li{
				margin-bottom:15px;
			}
		}
		.widget_fusion{
			.fusionentire{
				overflow:hidden;
				display:block;
				.fusionimg{
					display:block;
					margin-bottom:5px;
					float:left;
					width:130px;
					margin-right:10px;
				}
				.fusiontext{
					float:left;
					width:100px;
					font-size:13px;
					display:block;
					color:$brown;
				}				
			}
			.fusion-link{
				font-size:12px;
				font-weight:normal;
				text-transform:uppercase;
				color:$brown2;
			}
		}
		.widget_tagline h3 span{
			width:62px;
			margin-left:-41px;			
		}
		.widget_folyo h3 span{
			width:110px;
			margin-left:-65px;			
		}
		.widget_recent_entries h3 span{
			margin-left:-75px;
			width:130px;
		}
		.widget_social h3 span{
			width:102px;
			margin-left:-61px;
		}
		.widget_smashing h3 span{
			margin-left:-66px;
		}
		.widget_fusion h3 span{
			margin-left:-77px;
		}
	}
}
#footer{
	@include letterpress(0.5);
	font-size:14px;
	margin:$unit 0;
}
/*-------------- form ---------------*/
form{
	background:$lightbeige;
	width:600px;
	padding:30px 60px 30px 30px;
	.form-item{
		position:relative;
		margin-bottom:30px;
		&:before{
			@include stripes;
			content:" ";
			height:24px;
			width:360px;
			display:block;
			position:absolute;
			z-index:0;
			top:0;
		}
	}
	.wpcf7-text, textarea{
		@include border-radius(3px);
		border:none;
		@include box-emboss(0.8,0.35);
		@include box-shadow(0 1px 0 0 white, inset 0 1px 2px 0 black(0.15),inset 0 0 0px 1px #e2d0cc);
		position:relative;
	}
	.wpcf7-form-control-wrap{
		position:relative;
	}
	label{
		margin-bottom:10px;
		margin-top:1px;
		font-weight:bold;
		color:$beige;
		text-transform:uppercase;
		background:$lightbeige;
		display:inline-block;
		position:absolute;
		line-height:22px;
		z-index:10;
		padding:0 3px;
		margin-left:1px;
	}
	.wpcf7-text{
		height:24px;
		line-height:24px;
		display:block;
		width:340px;
		padding:8px 10px;
	}
	textarea{
		padding:10px;
		width:340px;
		height:150px;
	}
	input:focus, textarea:focus{
		outline:none;
		@include box-shadow(0 1px 0 0 white, inset 0 1px 2px 0 black(0.15),inset 0 0 0px 3px #e2d0cc);
	}
	span.wpcf7-not-valid-tip{
		position:absolute;
		top:-36px;
		background:none;
		border:none;
		color:$red;
		font-weight:bold;
		left:370px;
		font-size:16px;
	}
	.wpcf7-submit{
		color:white;
		background:$red;
		@include border-radius(30px);
		height:40px;
		padding:0 20px;
		line-height:40px;
		border:none;
		font-weight:bold;
	}
	div.wpcf7-response-output{
		margin:0;
		padding:15px;
		border:0;
		background:$lightgrey;
	}
	div.wpcf7-validation-errors{
		background:$red;
		color:white;
	}
	div.wpcf7-mail-sent-ok{
		background:$green;
	}
