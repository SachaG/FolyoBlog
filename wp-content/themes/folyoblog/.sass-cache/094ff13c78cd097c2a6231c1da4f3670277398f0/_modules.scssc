/*-------------- comments ---------------*/
#disqus_thread{
	margin:60px 0 !important;
	#dsq-global-toolbar{
		display:none;
	}
	#dsq-comments, .dsq-comments{
		list-style-type:none;
		border-top:1px solid $lightgrey !important;
		margin:30px 0 0 0 !important;
		padding:30px 0 0 0 !important;
	}
	.dsq-comment-child{
		margin-left:90px !important;
	}
	.dsq-comment, .dsq-comment:hover{
		border-bottom:1px solid $lightgrey;
		padding:0 $unit 30px 0 !important;
		margin-bottom:30px !important;
		.dsq-avatar{
			display:block;
/*			img{
				-webkit-border-radius: 60px !important;
				-moz-border-radius: 60px !important;
				border-radius: 60px !important;
				-webkit-box-shadow:none !important;
				-moz-box-shadow:none !important;
				box-shadow:none !important;
				margin:0 !important;
				padding:0 !important;
				height:60px !important;
				width:60px !important;
				border:none !important;
				background:none !important;
				@include transition-property(height);
				@include transition-property(width);
			}*/
			img{
				@include border-radius(60px);
				height:60px;
				width:60px;
			}
		}
		.dsq-comment-message{
			line-height:1.5 !important;
			font-size:16px !important;
		}
		.dsq-comment-body{
			padding-left:90px !important;
		}
	}
	.dsq-comment-is-collapsed,.dsq-comment-is-collapsed:hover{ 
		.dsq-avatar img{
			height:40px !important;
			width:40px !important;
		}
	}
	.dsq-comment-cite, .dsq-commenter-name{
		font-size:24px !important;
	}
	.dsq-comment-header{
		background:none;
		padding:0;
		border:none;
		line-height:1.2;
	}
	.dsq-depth-1{
		margin-left:90px !important;
	}
	#dsq-sort-by, .dsq-reply, #dsq-global-toolbar, #dsq-pagination{
		padding-right:$unit;
	}
	.dsq-trackbacks{
		margin-top:30px !important;
		border-top:1px solid $lightgrey;
		padding-top:30px !important;
		li{
			border-bottom:1px solid $lightgrey;
			padding:0 $unit 30px 0 !important;
			margin-bottom:30px !important;
			cite a{	
				font-style:normal;
			}
			.dsq-trackback-date{
				color:$beige;
				font-size:14px;
			}
		}
	}
